{
    "name": "horidaisuke/cakephp-aws-s3bucket",
    "description": "AWS S3Bucket plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "aws/aws-sdk-php": "^3.133",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12.8",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "S3Bucket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "S3Bucket\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
